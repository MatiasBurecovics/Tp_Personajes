{
    "openapi": "3.0.0",
    "info": {
      "title": "API de Tp-Personajes",
      "description": "API para gestionar información sobre películas, series y personajes relacionados con Disney.",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:5000/api"
      }
    ],
    "paths": {
      "/api/characters": {
        "get": {
          "summary": "Obtener todos los personajes",
          "responses": {
            "200": {
              "description": "Respuesta exitosa",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Personaje"
                    }
                  }
                }
              }
            }
          }
        },
        "post": {
          "summary": "Crear un nuevo personaje",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NuevoPersonaje"
                }
              }
            },
            "required": true
          },
          "responses": {
            "201": {
              "description": "Personaje creado exitosamente",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Personaje"
                  }
                }
              }
            },
            "400": {
              "description": "Datos de entrada inválidos"
            }
          }
        }
      },
      "/api/characters/{id}": {
        "get": {
          "summary": "Obtener un personaje por ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID del personaje",
              "schema": {
                "type": "string"
              },
              "required": true
            }
          ],
          "responses": {
            "200": {
              "description": "Respuesta exitosa",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Personaje"
                  }
                }
              }
            },
            "404": {
              "description": "Personaje no encontrado"
            }
          }
        },
        "put": {
          "summary": "Actualizar un personaje existente",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID del personaje a actualizar",
              "schema": {
                "type": "string"
              },
              "required": true
            }
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActualizarPersonaje"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "Personaje actualizado exitosamente",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Personaje"
                  }
                }
              }
            },
            "404": {
              "description": "Personaje no encontrado"
            }
          }
        },
        "delete": {
          "summary": "Eliminar un personaje",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID del personaje a eliminar",
              "schema": {
                "type": "string"
              },
              "required": true
            }
          ],
          "responses": {
            "200": {
              "description": "Personaje eliminado exitosamente"
            },
            "404": {
              "description": "Personaje no encontrado"
            }
          }
        }
      },
      "/api/movies": {
        "get": {
          "summary": "Obtener todas las películas o series",
          "responses": {
            "200": {
              "description": "Respuesta exitosa",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Pelicula"
                    }
                  }
                }
              }
            }
          }
        },
        "post": {
          "summary": "Crear una nueva película o serie",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NuevaPelicula"
                }
              }
            },
            "required": true
          },
          "responses": {
            "201": {
              "description": "Película o serie creada exitosamente",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Pelicula"
                  }
                }
              }
            },
            "400": {
              "description": "Datos de entrada inválidos"
            }
          }
        }
      },
      "/api/movies/{id}": {
        "get": {
          "summary": "Obtener una película o serie por ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID de la película o serie",
              "schema": {
                "type": "string"
              },
              "required": true
            }
          ],
          "responses": {
            "200": {
              "description": "Respuesta exitosa",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Pelicula"
                  }
                }
              }
            },
            "404": {
              "description": "Película o serie no encontrada"
            }
          }
        },
        "put": {
          "summary": "Actualizar una película o serie existente",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID de la película o serie a actualizar",
              "schema": {
                "type": "string"
              },
              "required": true
            }
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActualizarPelicula"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "Película o serie actualizada exitosamente",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Pelicula"
                  }
                }
              }
            },
            "404": {
              "description": "Película o serie no encontrada"
            }
          }
        },
        "delete": {
          "summary": "Eliminar una película o serie",
          "parameters": [
            {
              "name": "id",
              "in": "path"
            }
        ]
    
          